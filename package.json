{
  "name": "chat-next-app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.1.3",
  "scripts": {
    "dev": "next dev",
    "build-css": "tailwindcss build app/globals.css -o css/style.css",
    "build": "next build && npx tailwindcss -o css/style.css --minify",
    "deploy": "next build && mkdir public && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "next": "14.2.13",
    "react": "^18",
    "react-dom": "^18",
    "seedrandom": "^3.0.5",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18",
    "@types/seedrandom": "^3.0.8",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.2"
  }
